{
  "name": "charactorworks-grid-desktop",
  "productName": "CharactorWorks Grid Desktop",
  "description": "Register Windows Global Shortcut for CW",
  "keywords": [
    "CharactorWorks",
    "CW",
    "Global Shortcut",
    "desktop",
    "windows"
  ],
  "main": "./main.js",
  "version": "0.1.0",
  "author": "wuXin",
  "scripts": {
    "start": "electron .",
    "pack": "electron-packager . CharactorWorks-Grid-Desktop --win --out ./CharactorWorks-Grid-Desktop --arch=x64 --unpack * --app-version=0.2.0 --overwrite --icon=CGD.ico",
    "rebuild": "electron-rebuild -f -w yourmodule"
  },
  "devDependencies": {
    "electron": "^5.0.13",
    "electron-packager": "^15.1.0",
    "electron-rebuild": "^2.0.0",
    "electron-reload": "^1.5.0"
  },
  "dependencies": {}
}
